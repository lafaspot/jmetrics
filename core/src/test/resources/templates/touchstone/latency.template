       {
           "name": "Mail-Jedi-{{MetricClass}}-{{MethodName}}",
           "metric_type": "yamas",
           "threshold": "(test.expressionid > {{MaxDeviation}})",
           "environment": {
               "test":{
                        "filter": "$testCluster",
                        "group_by": true,
                        "type": "literal_or",
                        "tagk": "mlcluster"
                      }
           },
           "yamas_input_tags": {
               "time": {
                   "downsampler": {
                       "interval": "240s",
                       "aggregator": "sum"
                   },
                   "aggregator": "zimsum"
               },
               "explicitTags": true,
               "filters": [
                  {
                        "tags": [
                            {
                                "filter": "SUM",
                                "group_by": true,
                                "type": "literal_or",
                                "tagk": "_aggregate"
                            }
                        ],
                        "id": "f1"
                    }
               ],
               "metrics": [
                   {
                       "id": "latencycount",
                       "metric": "{{NameSpace}}.{{MetricClass}}.{{MethodName}}.Count",
                       "filter": "f1"
                   },
                   {
                       "id": "latency",
                       "metric": "{{NameSpace}}.{{MetricClass}}.{{MethodName}}",
                       "filter": "f1"
                   }
               ],
               "expressions": [
                   {
                       "id": "expressionid",
                       "expr": "latency / latencycount)"
                   }
               ]
           }
       },